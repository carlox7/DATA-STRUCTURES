  "license": "MIT",
  "engines": {
    "node": ">=0.12.6"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "jshint": "^2.9.1",
    "mocha": "^2.4.5"
  },
  "bugs": {
    "url": "https://github.com/johntimothybailey/chai-sorted/issues"
